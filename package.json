{
  "name": "next-ts-starter",
  "description": "Next Typescript Starter",
  "private": true,
  "author": "ismlhbb <ismail.habibi2104@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint ./src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{js,jsx,ts,tsx}",
    "lint:strict": "eslint --max-warnings=0 ./src/**/*.{js,jsx,ts,tsx,css,scss,md,json}",
    "tsc": "tsc",
    "prettier:check": "prettier -c ./src/**/*.{js,jsx,ts,tsx,css,scss,md,json}",
    "format": "prettier -w ./src/**/*.{js,jsx,ts,tsx,css,scss,md,json} --config ./.prettierrc.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "@tailwindcss/forms": "^0.3.4",
    "axios": "^0.24.0",
    "clsx": "^1.1.1",
    "next": "12.0.2",
    "next-seo": "^4.28.1",
    "nprogress": "^0.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "sass": "^1.43.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@types/node": "16.11.6",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.33",
    "@types/react-redux": "^7.1.20",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "autoprefixer": "^10.4.0",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.6",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "tailwindcss": "^2.2.19",
    "typescript": "4.4.4"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "./src/**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  }
}
